{
  "expo": {
    "name": "The Grey Tutor",
    "slug": "thegreytutor",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#2c3e50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thegreytutor.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2c3e50"
      },
      "package": "com.thegreytutor.app"
    },
    "web": {
      "bundler": "metro"
    },
    "main": "./App.tsx",
    "scheme": "thegreytutor",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    }
  }
}